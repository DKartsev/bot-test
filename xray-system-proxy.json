      }
    },
    {
      "tag": "http",
      "listen": "0.0.0.0",
      "port": 8080,
      "protocol": "http",
      "settings": {
        "allowTransparent": false
      }
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "protocol": "vless",
      "settings": {
        "vnext": [
