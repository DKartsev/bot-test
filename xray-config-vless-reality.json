        "address": "127.0.0.1"
      }
    }
  ],
  "outbounds": [
    {
      "tag": "vless-reality",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "193.233.115.178",
            "port": 443,
            "users": [
              {
                "id": "bd6ecf06-98c3-48a9-bf33-1d5ab7d11350",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "cloudflare.com:443",
          "xver": 0,
          "serverNames": [
            "cloudflare.com"
          ],
          "privateKey": "",
          "minClientVer": "",
          "maxClientVer": "",
