{
  "log": {
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log",
    "loglevel": "info"
  },
  "inbounds": [
    {
      "tag": "socks",
      "listen": "127.0.0.1",
      "port": 1080,
      "protocol": "socks",
      "settings": {
        "udp": true,
        "auth": "noauth"
      }
    }
  ],
  "outbounds": [
    {
      "tag": "vless-tls",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "193.233.115.178",
            "port": 443,
            "users": [
              {
                "id": "bd6ecf06-98c3-48a9-bf33-1d5ab7d11350",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "cloudflare.com",
          "allowInsecure": false
        }
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "outboundTag": "vless-tls",
        "domain": [
          "api.openai.com",
          "openai.com"
        ]
      },
      {
        "type": "field",
        "outboundTag": "direct",
        "domain": [
          "api.telegram.org",
          "telegram.org"
        ]
      }
    ]
  }
}
